
#include <bits/stdc++.h>
using namespace std;
int getlargest(int arr[],int n)
{
    int largestelement=0;
    for(int i=0;i<n;i++)
    {
        if(largestelement<arr[i])
        {
            largestelement=arr[i];
        }
    }
    return largestelement;
}
int main()
{
    int arr[]={5,8,20,15};
    cout<<getlargest(arr,4);
    return 0;
}
